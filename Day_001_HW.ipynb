   "source": [
    "# 請寫一個函式用來計算 Mean Square Error"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "def mean_sqr_error(y, y_pred):\n",
    "    mse = sum(pow(abs(y-y_pred),2))/len(y)\n",
    "    return mse"
   ]
  },
